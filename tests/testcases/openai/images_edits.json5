{
  // OpenAI Images Edits API 测试配置 - 严格对应 test_images_edits.py
  "provider": "openai",
  "endpoint": "/v1/images/edits",
  "base_params": {
    "model": "gpt-image-1.5",
    "prompt": "Replace the background with a blue sky."
  },
  "response_schema": "openai.ImageResponse",
  "tests": [
    // 基线测试
    {
      "name": "test_baseline",
      "description": "测试基线：gpt-image-1.5 单图 + prompt",
      "files": {
        "image": "test_assets/images/test_base.png"
      }
    },
    // 参数变体
    {
      "name": "test_param_response_format_url_dalle2",
      "description": "dall-e-2 支持 response_format=url",
      "params": {
        "model": "dall-e-2",
        "response_format": "url"
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "response_format"
    },
    {
      "name": "test_param_background_transparent",
      "description": "GPT image 支持 background 透明",
      "params": {
        "background": "transparent"
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "background"
    },
    {
      "name": "test_param_output_format_webp",
      "description": "GPT image 支持 output_format=webp",
      "params": {
        "output_format": "webp"
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "output_format"
    },
    {
      "name": "test_param_output_compression",
      "description": "GPT image 支持 output_compression (webp/jpeg)",
      "params": {
        "output_format": "webp",
        "output_compression": 80
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "output_compression"
    },
    {
      "name": "test_param_size_gpt_landscape",
      "description": "GPT image 尺寸横版",
      "params": {
        "size": "1536x1024"
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "size"
    },
    {
      "name": "test_param_n_multiple",
      "description": "生成多张图片 (n=2)",
      "params": {
        "n": 2
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "n"
    },
    {
      "name": "test_param_input_fidelity_high",
      "description": "输入忠实度（仅 GPT image 模型）",
      "params": {
        "input_fidelity": "high"
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "input_fidelity"
    },
    {
      "name": "test_param_mask_png",
      "description": "带 mask 的编辑",
      "files": {
        "image": "test_assets/images/test_base.png",
        "mask": "test_assets/images/test_base.png"
      },
      "unsupported_param": "mask"
    },
    {
      "name": "test_param_user",
      "description": "用户标识 user",
      "params": {
        "user": "user-123"
      },
      "files": {
        "image": "test_assets/images/test_base.png"
      },
      "unsupported_param": "user"
    }
  ]
}

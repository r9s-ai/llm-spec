{
  endpoint: "/v1/audio/transcriptions",
  schemas: {
    response: "openai.AudioTranscriptionResponse",
    stream_chunk: "openai.AudioStreamEvent",
  },
  base_params: {
  },
  tests: [
    {
      name: "baseline",
      description: "Baseline test: only required parameters",
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      baseline: true,
    },
    {
      name: "language",
      description: "Test language parameter",
      params: {
        language: "en",
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "language",
        value: "en",
      },
    },
    {
      name: "temperature",
      description: "Test temperature parameter",
      params: {
        temperature: 0.0,
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "temperature",
        value: 0.0,
      },
    },
    {
      name: "response_format",
      description: "Test response_format parameter (all formats)",
      variants: {
        response_format: [
          "json",
          "text",
          "verbose_json",
          "srt",
          "vtt",
        ],
      },
      params: {
        response_format: "$response_format",
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "response_format",
        value: "$response_format",
      },
    },
    {
      name: "prompt",
      description: "Test prompt parameter",
      params: {
        prompt: "Please transcribe in a friendly style.",
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "prompt",
        value: "Please transcribe in a friendly style.",
      },
    },
    {
      name: "chunking_strategy",
      description: "Test chunking_strategy variants",
      variants: {
        chunking_variant: [
          {
            variant_id: "auto",
            value: "auto",
          },
          {
            variant_id: "server_vad",
            value: {
              type: "server_vad",
            },
          },
        ],
      },
      params: {
        chunking_strategy: "$chunking_variant.value",
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "chunking_strategy",
        value: "$chunking_variant.value",
      },
    },
    {
      name: "include",
      description: "Test include=logprobs",
      params: {
        include: [
          "logprobs",
        ],
        response_format: "json",
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "include",
        value: [
          "logprobs",
        ],
      },
      required_fields: ["logprobs"],
    },
    {
      name: "known_speaker_names",
      description: "Test known_speaker_names + known_speaker_references",
      params: {
        known_speaker_names: [
          "speaker_a",
          "speaker_b",
        ],
        known_speaker_references: [
          "$asset_data_uri(assets/audio/speaker_a.mp3,audio/mpeg)",
          "$asset_data_uri(assets/audio/speaker_b.mp3,audio/mpeg)",
        ],
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "known_speaker_names",
        value: [
          "speaker_a",
          "speaker_b",
        ],
      },
    },
    {
      name: "timestamp_granularities",
      description: "Test timestamp_granularities with verbose_json",
      params: {
        response_format: "verbose_json",
        timestamp_granularities: [
          "word",
          "segment",
        ],
      },
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "timestamp_granularities",
        value: [
          "word",
          "segment",
        ],
      },
      required_fields: ["duration", "language", "segments", "words"],
    },
    {
      name: "stream",
      description: "Test stream=true",
      params: {
        stream: true,
      },
      stream: true,
      files: {
        file: "assets/audio/hello_en.mp3",
      },
      focus_param: {
        name: "stream",
        value: true,
      },
    },
  ],
}

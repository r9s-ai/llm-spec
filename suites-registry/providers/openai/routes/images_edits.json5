{
  endpoint: "/v1/images/edits",
  schemas: {
    response: "openai.ImageResponse",
    stream_chunk: "openai.ImageStreamEvent",
  },
  base_params: {
    prompt: "Replace the background with a blue sky.",
  },
  tests: [
    {
      name: "baseline",
      description: "Baseline test: gpt-image-1.5 single image + prompt",
      files: {
        image: "assets/images/test_base.png",
      },
      baseline: true,
    },
    {
      name: "response_format",
      description: "dall-e-2 supports response_format=url",
      params: {
        response_format: "url",
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "response_format",
        value: "url",
      },
    },
    {
      name: "background",
      description: "GPT image supports transparent background",
      params: {
        background: "transparent",
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "background",
        value: "transparent",
      },
    },
    {
      name: "output_format",
      description: "GPT image supports output_format=webp",
      params: {
        output_format: "webp",
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "output_format",
        value: "webp",
      },
    },
    {
      name: "output_compression",
      description: "GPT image supports output_compression (webp/jpeg)",
      params: {
        output_format: "webp",
        output_compression: 80,
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "output_compression",
        value: 80,
      },
    },
    {
      name: "size",
      description: "GPT image landscape size",
      params: {
        size: "1536x1024",
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "size",
        value: "1536x1024",
      },
    },
    {
      name: "n",
      description: "Generate multiple images (n=2)",
      params: {
        n: 2,
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "n",
        value: 2,
      },
    },
    {
      name: "input_fidelity",
      description: "Input fidelity (GPT image model only)",
      params: {
        input_fidelity: "high",
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "input_fidelity",
        value: "high",
      },
    },
    {
      name: "mask",
      description: "Edit with mask",
      params: {},
      files: {
        image: "assets/images/test_base.png",
        mask: "assets/images/test_base.png",
      },
      focus_param: {
        name: "mask",
        value: null,
      },
    },
    {
      name: "user",
      description: "User identifier",
      params: {
        user: "user-123",
      },
      files: {
        image: "assets/images/test_base.png",
      },
      focus_param: {
        name: "user",
        value: "user-123",
      },
    },
  ],
}

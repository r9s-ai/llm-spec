{
  endpoint: "/v1/embeddings",
  schemas: {
    response: "openai.EmbeddingResponse",
  },
  base_params: {
    input: "Hello, world!",
  },
  tests: [
    {
      name: "baseline",
      description: "Baseline test: only required parameters",
      baseline: true,
    },
    {
      name: "input",
      description: "Test input parameter variants",
      variants: {
        input_variant: [
          {
            variant_id: "string_array",
            value: [
              "Hello",
              "World",
            ],
          },
          {
            variant_id: "token_array",
            value: [
              [
                11,
                12,
                13,
                14,
              ],
            ],
          },
        ],
      },
      params: {
        input: "$input_variant.value",
      },
      focus_param: {
        name: "input",
        value: "$input_variant.value",
      },
    },
    {
      name: "encoding_format",
      description: "Test encoding_format parameter (base64)",
      params: {
        encoding_format: "base64",
      },
      focus_param: {
        name: "encoding_format",
        value: "base64",
      },
    },
    {
      name: "user",
      description: "Test user parameter (user identifier)",
      params: {
        user: "user-123",
      },
      focus_param: {
        name: "user",
        value: "user-123",
      },
    },
    {
      name: "dimensions",
      description: "Test dimensions parameter (V3 models only)",
      params: {
        dimensions: 512,
      },
      focus_param: {
        name: "dimensions",
        value: 512,
      },
    },
  ],
}

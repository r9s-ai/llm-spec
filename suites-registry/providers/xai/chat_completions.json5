{
  provider: "xai",
  endpoint: "/chat/completions",
  schemas: {
    response: "xai.ChatCompletionResponse",
  },
  base_params: {
    model: "grok-beta",
    messages: [
      {
        role: "user",
        content: "Hello",
      },
    ],
  },
  tests: [
    {
      name: "test_baseline",
      description: "Baseline test",
      is_baseline: true,
    },
    {
      name: "test_param_temperature",
      description: "Test temperature parameter",
      params: {
        temperature: 0.7,
      },
      test_param: {
        name: "temperature",
        value: 0.7,
      },
    },
    {
      name: "test_param_max_tokens",
      description: "Test max_tokens parameter",
      params: {
        max_tokens: 100,
      },
      test_param: {
        name: "max_tokens",
        value: 100,
      },
    },
  ],
}

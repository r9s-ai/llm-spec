{
  provider: "gemini",
  endpoint: "/v1beta/models/gemini-3-flash-preview:generateContent",
  schemas: {
    response: "gemini.GenerateContentResponse",
  },
  base_params: {
    contents: [
      {
        role: "user",
        parts: [{ text: "Say hello" }],
      },
    ],
  },
  // Gemini generation parameters typically live under generationConfig.
  param_wrapper: "generationConfig",
  required_fields: ["candidates[0].content.parts[0].text"],
  tests: [
    {
      name: "test_baseline",
      description: "Baseline test: preview model + minimal required parameters",
      is_baseline: true,
    },
    {
      name: "test_param_temperature",
      description: "Test generationConfig.temperature (image model override)",
      endpoint_override: "/v1beta/models/gemini-3-flash-image:generateContent",
      params: { temperature: 0.7 },
      test_param: { name: "generationConfig.temperature", value: 0.7 },
    },
    {
      name: "test_param_max_output_tokens",
      description: "Test generationConfig.maxOutputTokens (image model override)",
      endpoint_override: "/v1beta/models/gemini-3-flash-image:generateContent",
      params: { maxOutputTokens: 64 },
      test_param: { name: "generationConfig.maxOutputTokens", value: 64 },
    },
    {
      name: "test_param_top_p",
      description: "Test generationConfig.topP (image model override)",
      endpoint_override: "/v1beta/models/gemini-3-flash-image:generateContent",
      params: { topP: 0.9 },
      test_param: { name: "generationConfig.topP", value: 0.9 },
    },
    {
      name: "test_param_top_k",
      description: "Test generationConfig.topK (image model override)",
      endpoint_override: "/v1beta/models/gemini-3-flash-image:generateContent",
      params: { topK: 20 },
      test_param: { name: "generationConfig.topK", value: 20 },
    },
    {
      name: "test_param_candidate_count",
      description: "Test generationConfig.candidateCount (image model override)",
      endpoint_override: "/v1beta/models/gemini-3-flash-image:generateContent",
      params: { candidateCount: 2 },
      test_param: { name: "generationConfig.candidateCount", value: 2 },
    },
    {
      name: "test_param_stop_sequences",
      description: "Test generationConfig.stopSequences (image model override)",
      endpoint_override: "/v1beta/models/gemini-3-flash-image:generateContent",
      params: { stopSequences: ["END"] },
      test_param: { name: "generationConfig.stopSequences", value: ["END"] },
    },
  ],
}
